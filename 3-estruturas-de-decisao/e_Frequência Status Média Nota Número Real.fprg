            <declare name="frequencia" type="Real" array="False" size=""/>
            <output expression="&quot;Digite a m&#233;dia da nota do aluno: &quot;" newline="True"/>
            <input variable="media"/>
            <output expression="&quot;Digite a frequ&#234;ncia do aluno: &quot;" newline="True"/>
